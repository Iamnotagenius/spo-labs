function sum(arr as int(), count) as int
    dim i, sum as long
    i = 0;
    sum = 0;
    do
        sum = sum + arr(i);
        i = i + 1;
    loop until i >= count
    sum;
end function

function add(a, b)
    a + b;
end function

function fib(n) as ulong
    dim prev, next as ulong
    prev = 0;
    next = 1;
    while n > 1
        prev = prev + next;
        next = prev + next;
        n = n - 2;
    wend
    if n == 0 then
        prev;
    else
        next;
    end if
end function

function bubbleSort(arr as int(), length as ulong)
    dim i as int
    i = 0;
    while i < length
        dim j as int
        j = 0;
        while j < length
            if arr(j) > arr(j + 1) then
                dim temp as int
                temp = arr(j);
                arr(j) = arr(j + 1);
                arr(j + 1) = temp;
            end if
            j = j + 1;
        wend
        i = i + 1;
    wend
end function

function mul(a, b)
    a * b;
end function
