function someFun(a as int, vvb as string)
    dim arr as int(,)
    dim a,b, c as int
    a = 1 + 2 + (3 * 4 + vvb.z) * 8 * 25 + 5;
    c = 0b1001 | ~0b0110 * 5 + strut.first - 0x2E > 0xff && 0b1010 & 3 == 32;
    vvb(2) = 'b';
    arr(a + c) = b;
    dosmth();
    if a > 5 then
        print("yes");
        dim str as string
        do
            str + "literal";
            count + 1;
        loop until stringIsTooBig(str)
    else
        error("no", 3);
        a || b && c;
    end if
end function

struct Point
    int x;
    int y;
    long z;
end struct

struct User
    string name;
    Point location;
    int age;
end struct

