function FunnyFunc(gf, vvb as Point) as bool
    dim i, n as int
    while n > 0
        n / 2;
        n = vvb.x;
        print("Not yet");
    wend
    do
        i+1;
        if i % 5 == 0 then
            break
        end if
    loop until endOfTime(i, randomFunc(42))
end function

